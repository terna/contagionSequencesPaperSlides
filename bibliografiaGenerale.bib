@article{Krichene2017,
	Abstract = {This paper aims mainly at building artificial stock markets with different maturity levels by modeling information asymmetry and herd behavior. The developed artificial markets are multi-assets, order-driven and populated by agents having heterogeneous behaviors and information. Agents are defined by their information and their herd behavior levels. Agents trade multiple risky assets based on their wealth, their behaviors and their available information which spread among multiple behavioral networks. In a novel contribution to artificial stock markets literature, agents' behaviors modeling is mixed with social network simulation to reproduce different degrees of information asymmetry and herd behavior based on several assortative topologies. Several simulations validated the proposed model since univariate and multivariate stylized facts were reproduced both for mature and immature stock markets. The proposed artificial stock market can be considered as a first step toward decision and simulation tools for optimal management, strategy analysis and predictions evolution of immature stock markets.},
	Author = {Krichene, Hazem and El-Aroui, Mhamed-Ali},
	Date-Added = {2017-02-20 21:25:58 +0000},
