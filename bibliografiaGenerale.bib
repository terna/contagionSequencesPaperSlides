@article{PUU201997,
	Abstract = {There have been some recent attempts to combine Cournot and Bertrand duopolies in one single model. Unfortunately, these attempts do not work. A commodity cannot be homogenous and non-homogenous at the same time. It is always the consumers, who decide whether they perceive competing products as identical or as different brands for which they are willing to pay different prices. There is, of course, nothing that forbids the coexistence of both such consumer groups. Neither is there any obstacle for the competing sellers to sell to both markets. Then we only need an old idea from economic theory, i.e., price discrimination, to rectify the logic. By this the challenging combination idea comes on a stable footing. The model also results in some interesting mathematical facts, such as mulistability and coexistence of attractors.},
	Author = {Tonu Puu and Fabio Tramontana},
	Date-Added = {2019-05-31 17:15:26 +0200},
	Date-Modified = {2019-05-31 17:15:59 +0200},
	Doi = {https://doi.org/10.1016/j.chaos.2019.05.026},
	Issn = {0960-0779},
	Journal = {Chaos, Solitons \& Fractals},
	Keywords = {Cournot--Bertrand, Price discrimination, Global analysis, Local bifurcations},
	Pages = {97 - 107},
	Title = {Can Bertrand and Cournot oligopolies be combined?},
	Url = {http://www.sciencedirect.com/science/article/pii/S0960077919301900},
