@article{sulis2020,
	Abstract = {Agent-based approaches have been known to be appropriate as systems and methods in medical administration in recent years. The increased attention to processes led to the recent growth of Business Process Management discipline, which quite exclusively adopt discrete-event modeling and simulation. This paper proposes a medical agent-oriented decision support system to integrate the achievements from management science, agent-based modeling, and artificial intelligence. In particular, we performed a practical application concerning a hospital emergency department medical system. We adopt the widely used multi-agent programmable modeling environment NetLogo. First, we demonstrated the ability to perform a clear representation of healthcare processes where agents (i.e., patients and hospital staff) operate in a 3D environment. This model allows performing a traditional what-if scenario analysis. Second, we explore how performing intelligent management of patients by applying genetic algorithms to find the criteria for the selection process of the subjects in the admission procedure. The results are encouraging towards a more extensive application of agent-oriented methodologies in healthcare management.},
	Author = {Sulis, Emilio and Terna, Pietro and Di Leva, Antonio and Boella, Guido and Boccuzzi, Adriana},
	Da = {2020/08/02},
