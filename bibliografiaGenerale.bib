	Issn = {0036-8075},
	Journal = {Science},
	Number = {6419},
	Pages = {1140--1144},
	Publisher = {American Association for the Advancement of Science},
	Title = {A general reinforcement learning algorithm that masters chess, shogi, and Go through self-play},
