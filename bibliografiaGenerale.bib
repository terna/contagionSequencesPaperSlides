	Issn = {1467-6419},
	Journal = {Journal of Economic Surveys},
	Keywords = {General Identification Method (GIM), Imperfect competition, Market power, New Empirical Industrial Organization (NEIO), Production-Theoretic Approach (PTA), Oligopsony},
