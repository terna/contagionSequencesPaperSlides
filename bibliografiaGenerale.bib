	Doi = {10.1002/sys.21387},
	Issn = {1520-6858},
	Journal = {Systems Engineering},
	Keywords = {agent-based model, complex adaptive systems, critical infrastructure system, resilience measures, capacitated networks},
	Pages = {n/a--n/a},
